<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<EnableWindowsTargeting>true</EnableWindowsTargeting>
		<LangVersion>latest</LangVersion>
		<PlatformTarget>x64</PlatformTarget>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
		<!--<CompilerGeneratedFilesOutputPath>GeneratedFiles</CompilerGeneratedFilesOutputPath>-->

	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Resources\Images\Information_16.png" />
	  <None Remove="Resources\Images\Information_32.png" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Resources\Images\Information_16.png" />
	  <EmbeddedResource Include="Resources\Images\Information_32.png" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Nice3point.Revit.Api.RevitAPI" Version="2025.0.2" />
	  <PackageReference Include="Nice3point.Revit.Api.RevitAPIUI" Version="2025.0.2" />
	  <PackageReference Include="Nice3point.Revit.Api.AdWindows" Version="2025.0.2" />
	  <PackageReference Include="Scotec.Revit" Version="1.0.0" />
	  <PackageReference Include="Scotec.Revit.LoadContext" Version="1.0.0" />

	</ItemGroup>

	<ItemGroup>
	  <None Update="appsettings.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>

</Project>
